<!DOCTYPE HTML>
<html lang="pt-BR">
<head>
    <title>CYBER STOCK - KITAB INFORMÁTICA LTDA - ME</title>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <meta name="description" content="Site para gerenciar Produtos - Desenvolvido pela empresa KITAB INFORMÁTICA LTDA - ME." />
    <meta name="litmsProfileName" content="homepage-guest-frontend" />
    <meta property="og:site_name" content="CYBER STOCK" />
    <meta property="og:title" content="CYBER STOCK - KITAB INFORMÁTICA LTDA - ME" />
    <meta property="og:description" content="Site para gerenciar Produtos - Desenvolvido pela empresa KITAB INFORMÁTICA LTDA - ME." />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="https://kitab.com.br" />
    <!--<meta property="og:image" content="https://kitab.com.br/images/admin/favicon.png" />-->
    <link rel="canonical" href="https://kitab.com.br" />
    <link rel="stylesheet" href="~/lib/fontawesome-free/css/all.min.css" />
    <link rel="stylesheet" href="~/css/adminlte.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" />
</head>
<body class="hold-transition sidebar-mini">
    <!-- BEGIN Site Wrapper -->
    <div class="wrapper">
        <!-- BEGIN Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- BEGIN Left Navbar Links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Project">O Projeto</a>
                </li>
            </ul>
            <!-- END Left Navbar Links -->
        </nav>
        <!-- END Navbar -->
        <!-- BEGIN Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- BEGIN Brand Logo -->
            <a class="brand-link" asp-area="" asp-controller="Home" asp-action="Index">
                <span class="brand-text font-weight-light">CYBER STOCK</span>
            </a>
            <!-- END Brand Logo -->
            <!-- BEGIN Sidebar -->
            <div class="sidebar">
                <!-- BEGIN Sidebar user (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="~/img/users/user1-128x128.jpg" class="img-circle elevation-2" alt="Imagem do Usuário">
                    </div>
                    <div class="info">
                        <a href="#" class="d-block">Usuário Anônimo</a>
                    </div>
                </div>
                <!-- END Sidebar user (optional) -->
                <!-- BEGIN Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">
                                <i class="nav-icon fas fa-star"></i><p>Home</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Project">
                                <i class="nav-icon fas fa-code"></i><p>O Projeto</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Products" asp-action="Index">
                                <i class="nav-icon fas fa-archive"></i><p>Produtos</p>
                            </a>
                        </li>
                    </ul>
                </nav>
                <!-- END Sidebar Menu -->
            </div>
            <!-- END Sidebar -->
        </aside>

        <!-- BEGIN Content Wrapper -->
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <!-- END Content Wrapper -->

        <footer class="main-footer">
            <div class="float-right d-none d-sm-block"><b>Version</b> 3.0.5</div>
            <strong>&copy; 2014-2022 <a href="http://adminlte.io">AdminLTE.io</a>.</strong>&nbsp;-&nbsp;
            <i>Todos os direitos reservados</i>&nbsp;-&nbsp;
            <span>Powered by <a href="https://kitab.com.br" target="_blank">KITAB Informática Ltda - ME</a></span>
        </footer>
    </div>
    <!-- END Site Wrapper -->
    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/adminlte.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
